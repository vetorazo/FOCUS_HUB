<%= simple_form_for([listing, booking]) do |f| %>
  <%= f.input :start_date,
              as: :string,
              label: "Start Date",
              input_html: { data: {controller: "datepicker"} }  %>
  <%= f.input :end_date,
              as: :string,
              label: "End Date",
              input_html: { data: {controller: "datepicker"} }  %>
  <%= f.button :submit, "Book Now" %>
<% end %>
